                <div id="icono-nav">
                    <i class="fa-duotone fa-bars"></i>
                </div>
                <!--Iconos Redes Sociales-->
                <div class="redes">
                    <a href="#"><i class="fa-brands fa-youtube"></i></a>
                    <a href="#"><i class="fa-brands fa-square-facebook"></i></a>
                    <a href="#"><i class="fa-brands fa-instagram"></i></a>
